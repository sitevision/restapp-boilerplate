{
  "name": "restapp-boilerplate",
  "version": "1.0.0",
  "description": "A RESTApp boilerplate",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitevision/restapp-boilerplate.git"
  },
